<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RatingView">
        <attr name="displayMode" format="enum">
            <enum name="viewMode" value="0"/>
            <enum name="editableMode" value="1"/>
            <enum name="editMode" value="2"/>
        </attr>
        <attr name="rating" format="integer"/>
    </declare-styleable>

    <declare-styleable name="StockView">
        <attr name="stock" format="integer"/>
        <attr name="isLocked" format="boolean"/>
    </declare-styleable>
</resources>